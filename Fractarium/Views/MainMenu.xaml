<UserControl xmlns="https://github.com/avaloniaui"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:ui="clr-namespace:Fractarium.Views;assembly=Fractarium"
			 mc:Ignorable="d"
			 d:DesignWidth="800"
			 d:DesignHeight="150"
			 x:Class="Fractarium.Views.MainMenu">
	<StackPanel Orientation="Horizontal"
				Spacing="5">
		<Button Padding="10"
				Margin="10">Render</Button>
		<TabControl>
			<TabItem Header="Parameters">
				<ui:ParameterTab/>
			</TabItem>
			<TabItem Header="Color"></TabItem>
		</TabControl>
	</StackPanel>
</UserControl>